{
  "name": "mumble-node",
  "version": "0.1.0",
  "description": "Node.js library for interacting with Mumble servers",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "build:proto": "pbjs -t static-module -w commonjs -o src/proto/mumble.js src/proto/Mumble.proto src/proto/MumbleUDP.proto && pbts -o src/proto/mumble.d.ts src/proto/mumble.js",
    "example:cli": "npx ts-node examples/cli.ts",
    "example:web": "npx ts-node examples/web-client/server.ts"
  },
  "keywords": [
    "mumble",
    "voip",
    "voice",
    "chat"
  ],
  "license": "MIT",
  "devDependencies": {
    "@types/blessed": "^0.1.27",
    "@types/node": "^20.10.0",
    "protobufjs-cli": "^2.0.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.0"
  },
  "dependencies": {
    "@discordjs/opus": "^0.10.0",
    "@discordjs/voice": "^0.19.0",
    "@evan/opus": "^1.0.3",
    "@mastra/node-speaker": "^0.1.0",
    "@roamhq/wrtc": "^0.9.1",
    "@snazzah/davey": "^0.1.9",
    "@types/ws": "^8.18.1",
    "blessed": "^0.1.81",
    "discord.js": "^14.25.1",
    "mic": "^2.1.2",
    "opusscript": "^0.1.1",
    "protobufjs": "^7.2.6",
    "tweetnacl": "^1.0.3",
    "ws": "^8.19.0"
  }
}
